 vnd <instruction>plas zue ainem <tool en="spit" commodity="Q20181361">spicz</tool> ein vnd das es zue dem andern auss <pb xml:id="Bs1_105r" n="105r"/> gee</instruction>
 vnd <instruction>thue <ingredient en="spice" commodity="Q42527">gewuercz</ingredient> darunder</instruction>
 vnd <instruction>thue es wider In die <tool en="eggshell" commodity="Q2731253">schalen</tool></instruction>
 vnd <instruction>stoeß an <tool en="spit" commodity="Q20181361">spisslein</tool></instruction>
 vnd <instruction>prat sy das sy Ir genuog haben</instruction>
