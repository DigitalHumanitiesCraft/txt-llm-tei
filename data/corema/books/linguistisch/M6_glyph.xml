				<lb n="N026"/><del>?</del> <g ref="#slong">s</g>ey<g ref="#combuml"/>dens <del>auf</del> in das <del>k??</del> kar auf
				<lb n="N027"/>den <abbr><ex>per</ex><am><g ref="#pbardes_per"/></am>tra<ex>n</ex><am><g ref="#bar_n"/></am></abbr> vnd mit <abbr>aine<ex>m</ex><am><g ref="#bar_m"/></am></abbr> <abbr>lo<g ref="#combuml"/>ff<ex>e</ex><am><g ref="#bar_e"/></am>l</abbr>
			
			<pb xml:id="M6_077r" n="077r"/>
			
				<lb n="N001"/>wol <abbr>ru<g ref="#combuml"/>re<ex>n</ex><am><g ref="#bar_n"/></am></abbr> durcheinander darnach ko<g ref="#slong">s</g>t
				<lb n="N002"/>es ob  es gerecht <g ref="#slong">s</g>ey<g ref="#combuml"/> an dem <g ref="#slong">s</g>alcz vnd an <abbr>de<ex>m</ex><am><g ref="#bar_m"/></am></abbr>
				<lb n="N003"/>pfeffer darnach nym a<g ref="#inodot">i</g>n <g ref="#slong">s</g>cho<g ref="#combuml"/>ns ha<g ref="#combuml"/>fflein
				<lb n="N004"/>vnd thue dy <abbr><g ref="#slong">s</g>al<g ref="#slong">s</g><g ref="#slong">s</g><ex>e</ex><am><g ref="#bar_e"/></am>n</abbr> darein vnd mach das
				<lb n="N005"/><del>?</del> ha<g ref="#combuml"/>fflein wolzu vnd <g ref="#slong">s</g>ecz es In ain <abbr>ki<g ref="#slong">s</g>te<ex>n</ex><am><g ref="#bar_n"/></am></abbr>
				<lb n="N006"/>oder almar das nicht ame<g ref="#slong">s</g><g ref="#slong">s</g>en darzu <abbr>cho<g ref="#combuml"/>me<ex>n</ex><am><g ref="#bar_n"/></am></abbr>
				<lb n="N007"/><abbr>mu<g ref="#combuml"/>ge<ex>n</ex><am><g ref="#bar_n"/></am></abbr> al<g ref="#slong">s</g>o hat man ain guet <abbr><g ref="#slong">s</g>al<g ref="#slong">s</g><g ref="#slong">s</g><ex>e</ex><am><g ref="#bar_e"/></am>n</abbr> <abbr><ex>etc.</ex><am><g ref="#etcamp"/></am></abbr></seg><seg>
				<lb n="N008"/><abbr>It<ex>e</ex><am><g ref="#bar_e"/></am>m</abbr> ca<g ref="#slong">s</g>ia<g ref="#slong">s</g>i<g ref="#slong">s</g>tula au<g ref="#slong">s</g>zu <abbr>z<g ref="#inodot">i</g>ech<ex>e</ex><am><g ref="#bar_e"/></am>n</abbr>
				<lb n="N009"/>Nempt <abbr>zu<ex>m</ex><am><g ref="#bar_m"/></am></abbr> <abbr>er<g ref="#slong">s</g>t<ex>e</ex><am><g ref="#bar_e"/></am>n</abbr> vnd <add>be</add><g ref="#slong">s</g>chney<g ref="#combuml"/>t von der
				<lb n="N010"/>ca<g ref="#slong">s</g>ia dy<g ref="#combuml"/> <abbr>rindt<ex>e</ex><am><g ref="#bar_e"/></am>n</abbr> vnd holcz herab vnd
				<lb n="N011"/>nempt das Inwenig das <g ref="#slong">s</g>cheyblig
				<lb n="N012"/>i<g ref="#slong">s</g>t vnd waicht es v<g ref="#combuml"/>ber nacht In ain
				<lb n="N013"/><abbr>ror<ex>e</ex><am><g ref="#bar_e"/></am>l</abbr> oder <abbr>di<g ref="#slong">s</g>t<ex>e</ex><am><g ref="#bar_e"/></am>l</abbr> wa<g ref="#slong">s</g><g ref="#slong">s</g>er oder In ain <note xml:lang="de">Senkrechter Strich vor 'r' als Wortgrenze?</note><note xml:lang="en">Vertical bar in front of 'r' as word boundary.</note><w>ra<g ref="#dbloblhyph">=</g>
				<lb n="N014"/>inual</w> vnd nempt ain <abbr><ex>libra</ex><am><g ref="#lbbar"/></am></abbr> ca<g ref="#slong">s</g><g ref="#slong">s</g><g ref="#inodot">i</g>a auf ain
				<lb n="N015"/>mal vnd <g ref="#slong">s</g>o es <add>dy<g ref="#combuml"/></add> nacht gewaicht <g ref="#inodot">i</g><g ref="#slong">s</g>t <g ref="#slong">s</g>o <w>ne<g ref="#dbloblhyph">=</g>
				<lb n="N016"/>mpt</w> an dem <abbr>morge<ex>n</ex><am><g ref="#bar_n"/></am></abbr> ain <g ref="#slong">s</g>aubers tuech <abbr>vn<ex>d</ex><am><g ref="#bar_d"/></am></abbr>
				<lb n="N017"/>trey<g ref="#combuml"/>bt es mit ainem <abbr>lo<g ref="#combuml"/>ff<ex>e</ex><am><g ref="#bar_e"/></am>l</abbr> durch das tuech
