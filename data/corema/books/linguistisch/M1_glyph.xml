			<pb xml:id="M1_118r" n="118r"/>
			
				<lb n="N001"/><hi style="heading"><hi>V</hi>ermerk ein gu<g ref="#combuml"/>ede ku<g ref="#combuml"/>n<g ref="#slong">s</g>t von <abbr>vng<g ref="#rrot">r</g>i<g ref="#slong">s</g>ch<ex>e</ex><am><g ref="#bar_e"/></am>n</abbr>
				<lb n="N002"/><abbr>od<ex>er</ex><am><g ref="#combcomma_er"/></am></abbr> <abbr>pehamy<g ref="#combuml"/><g ref="#slong">s</g>ch<ex>e</ex><am><g ref="#bar_e"/></am>n</abbr> e<g ref="#slong">s</g><g ref="#slong">s</g>en zw<g ref="#combuml"/>kochen dy gu<g ref="#combuml"/>et <g ref="#slong">s</g>ein</hi><seg>
				<lb n="N003"/><hi style="heading"><abbr>It<ex>e</ex><am><g ref="#bar_e"/></am>m</abbr> e<g ref="#inodot">i</g>n karpfen Im fu<g ref="#combuml"/>rhe<g ref="#slong">s</g>s 
				<lb n="N004"/>vnd e<g ref="#inodot">i</g>n fa<g ref="#slong">s</g>an<note xml:lang="de">Fehler? Gemeint vermutlich 'fasten'.</note><note xml:lang="en">Error? Should probably read 'fasten'.</note> p<g ref="#rrot">r</g>u<g ref="#combuml"/>e</hi>
